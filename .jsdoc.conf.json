{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "recurseDepth": 10,
  "source": {
    "include": ["./src/view"],
    "includePattern": "\\.(vue|js)$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": ["node_modules/jsdoc-vuejs"],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": true,
    "useLongnameInNav": false,
    "showInheritedInNav": true
  },
  "opts": {
    "destination": "./docs",
    "encoding": "utf8",
    "private": true,
    "recurse": true
  }
}
